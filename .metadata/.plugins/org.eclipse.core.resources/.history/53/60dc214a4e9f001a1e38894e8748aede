
public class Chapter08 {
	public static void main(String[] args) {
		int number = 1;
		while (number < 5) {
			number *= number;
			System.out.println(number);
		}
		
		int[] array = { 6, 1, 15, 4 };
		for (int i = 0; i <= array.length; i++) {
			System.out.println(i);
		}
		
		for (int val : array) {
			if (val % 2 == 0) {
				continue;
			}
			System.out.println(val);
		}
	}
}
